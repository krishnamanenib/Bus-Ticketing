					<xs:element name="PRItmNbr" type="xs:string" nillable="true"
						minOccurs="0">
						<xs:annotation>
							<xs:documentation>The R3 generated number which associated a
								given Style/Color with a Purchase Order (PO) Header. The system
								generated number takes the form of a six character value which
								begins with 000010 and increments higher in units of 10. Example
								Value: 000010, 000020, 000030, etc. MDR ID: 2038
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PlanMoDt" type="xs:string" nillable="true"
						minOccurs="0">
